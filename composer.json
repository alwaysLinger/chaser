{
  "name": "yylh/chaser",
  "type": "project",
  "description": "simple tcp framework",
  "keywords": [
    "event-driven",
    "tcp",
    "framework",
    "multi-process",
    "async"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.4.0",
    "ext-event": ">=2.5.7",
    "symfony/console": "^5.3",
    "php-di/php-di": "^6.3",
    "league/pipeline": "^1.0",
    "monolog/monolog": "^2.3",
    "pimple/pimple": "^3.4",
    "symfony/finder": "^5.3",
    "illuminate/support": "^8.62"
  },
  "autoload": {
    "psr-4": {
      "Al\\Chaser\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "config": {
    "optimize-autoloader": true
  }
}
